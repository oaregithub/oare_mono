{
  "name": "@oare/backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp -r src/api/sql build/src/api",
    "postbuild": "ef-tspm",
    "start": "rm -rf build/@oare && cp -r @oare build && cp -r @oare node_modules && NODE_ENV=production node ./build/src/server.js",
    "lint": "eslint src/*.ts src/**/*.ts",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register ./src/server.ts --respawn --transpileOnly ",
    "dev:3": "DB_VERSION=0.3 npm run dev",
    "test": "jest",
    "test:log": "NODE_ENV=log yarn test",
    "migrate:make": "knex migrate:make --migrations-directory knex_migrations -x ts",
    "migrate:latest": "knex migrate:latest --migrations-directory knex_migrations",
    "migrate:rollback": "knex migrate:rollback --migrations-directory knex_migrations",
    "migrate:up": "knex migrate:up --migrations-directory knex_migrations",
    "migrate:down": "knex migrate:down --migrations-directory knex_migrations"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@ef-carbon/tspm": "^2.2.5",
    "@types/express": "^4.17.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-jest": "^26.6.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.6.2",
    "prettier": "^2.1.1",
    "superagent": "^6.1.0",
    "supertest": "^6.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "jison": "git+https://github.com/oaregithub/jison.git",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.153",
    "@types/luxon": "^1.25.0",
    "@types/mailgun-js": "^0.22.11",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "crypto-random-string": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "lodash": "^4.17.15",
    "luxon": "^1.25.0",
    "mailgun-js": "^0.22.0",
    "mysql": "^2.18.1",
    "uuid": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
